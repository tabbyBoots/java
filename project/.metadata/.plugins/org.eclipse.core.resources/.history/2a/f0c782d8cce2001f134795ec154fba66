package topic02;

import java.util.Scanner;

public class homework01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		print5 p=new print5();
		p.print5(100);
		
		
		
			
		System.out.print("input num:\t");
		
		Scanner s= new Scanner(System.in);
		int num1 = s.nextInt();
		System.out.print("num1:\t"+ num1 +"\n");
		
		System.out.print("input num:\t");

		int num2 = s.nextInt();
		System.out.print("num2:\t"+ num2 +"\n");
		
		System.out.print("input num:\t");
	
		int num3 = s.nextInt();
		System.out.print("num3:\t"+ num3+"\n");
		
		int numTmp=0;

		if(num1>num2) {
			numTmp=num1;
			num1=num2;
			num2=numTmp;
			//System.out.print(num1+ "\t"+num2+"\n");	
		}
		
		if(num2>num3) {
			numTmp=num2;
			num2=num3;
			num3=numTmp;
			System.out.print(num1+ "\t"+num2+"\n");
		}
		
		System.out.print(num1+ "\t"+num2+"\t"+num3+"\n");
		System.out.print("END");
	
	}

}
