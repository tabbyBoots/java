package topic01;

public class LongType {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		long value;
		value = 15165456546544165L;
		System.out.println("Result:\t"+value +"\n\n");
		
		int days =32768;
		System.out.println("Result:\t"+days +"\n\n");
		
		int val1=20;
		System.out.println("decimal: "+ val1 +"\n");
		
		val1 = 0x30;
		System.out.println("0x16: "+ val1 +"\n");
		
		
		
		
	}

}
