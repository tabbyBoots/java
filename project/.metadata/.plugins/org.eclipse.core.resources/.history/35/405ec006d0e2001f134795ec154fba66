package topic02;

import java.util.Scanner;

public class homework01 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		print5 p=new print5();
		p.print5(100);
		
		
		
			
		System.out.print("input num:\t");
		
		Scanner s= new Scanner(System.in);
		int num1 = s.nextInt();
		
		System.out.print("input num:\t");

		int num2 = s.nextInt();
		
		System.out.print("input num:\t");
	
		int num3 = s.nextInt();
		
		int ary[]= {num1,num2,num3};//{30,20,10}
		
		int numSwap=0;
		int srtAry[]= {0,0,0};
				
		numSwap=num1;
		for(int i=1;i<2;i++) {
			if(numSwap>ary[i]) {
				srtAry[i]=numSwap;
				srtAry[i-1]=ary[i];			
			}
		}
		
		System.out.print(num1+ "\t"+num2+"\t"+num3+"\n");
		System.out.print("END");
	
	}

}
